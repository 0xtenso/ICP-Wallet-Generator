{
  "name": "icp-wallet-generator",
  "version": "1.0.0",
  "description": "A comprehensive ICP wallet generator with Internet Identity integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js cli",
    "dev": "node --watch src/index.js",
    "test": "node src/test.js",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:js": "node build.js",
    "wallet": "node test-wallet.js",
    "wallet:quick": "node src/index.js generate",
    "wallet:batch": "node src/index.js batch 5",
    "cli": "node src/index.js cli"
  },
  "keywords": [
    "internet-computer",
    "icp",
    "wallet",
    "cryptocurrency",
    "blockchain",
    "dfinity",
    "principal",
    "identity"
  ],
  "author": "ICP Wallet Generator",
  "license": "MIT",
  "dependencies": {
    "@dfinity/agent": "^2.0.0",
    "@dfinity/auth-client": "^2.0.0",
    "@dfinity/candid": "^2.0.0",
    "@dfinity/identity": "^2.0.0",
    "@dfinity/identity-secp256k1": "^2.0.0",
    "@dfinity/ledger-icp": "^2.0.0",
    "@dfinity/principal": "^2.0.0",
    "@dfinity/utils": "^2.0.0",
    "@noble/ed25519": "^2.3.0",
    "@noble/secp256k1": "^2.3.0",
    "bip39": "^3.1.0",
    "chalk": "^5.3.0",
    "crypto-js": "^4.2.0",
    "inquirer": "^9.2.11",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
